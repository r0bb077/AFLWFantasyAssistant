{
  "name": "AFLW Fantasy Assistant",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": { "16": "images/icon16.png", "48": "images/icon48.png", "128": "images/icon128.png" },
  "action": {},
  "permissions": ["alarms", "tabs", "scripting", "storage"],
  "host_permissions": [
    "http://api.afl.com.au/",
    "https://api.afl.com.au/",
    "https://www.aflwfantasy.com.au/*",
    "https://aflwfantasy.com.au/*"
  ]
}
